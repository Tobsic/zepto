(define (sub y z)
  (- y z))

(define (add y z)
  (+ y z))

(define (pow y z)
  (xp y z))

(define (append y z)
  (++ y z))

(define (mappend x y z)
  (++ x (++ y z)))

(define (add a b)
  (+ (id a) (id b)))

(define (id i) i)

(define (true) #t)
(define (false) #f)
